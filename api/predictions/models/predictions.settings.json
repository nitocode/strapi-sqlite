{
  "kind": "collectionType",
  "collectionName": "predictions",
  "info": {
    "name": "Predictions"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "babyName": {
      "type": "string",
      "required": true
    },
    "date": {
      "required": true,
      "max": 31,
      "default": 1,
      "unique": false,
      "type": "integer",
      "min": 1
    },
    "height": {
      "type": "integer",
      "required": true,
      "max": 65,
      "min": 35,
      "default": 35
    },
    "weight": {
      "type": "decimal",
      "required": true,
      "max": 5,
      "min": 2,
      "default": 2
    },
    "isGirlForm": {
      "type": "boolean",
      "required": true
    },
    "identified": {
      "type": "boolean",
      "default": false,
      "required": false,
      "private": true
    },
    "code": {
      "type": "string",
      "required": true,
      "maxLength": 8,
      "minLength": 8,
      "regex": "^[A-Z0-9]{8}$",
      "private": true
    }
  }
}
